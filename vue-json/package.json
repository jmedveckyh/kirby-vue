{
  "name": "kirby-vue-starterkit",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build && node -e \"require('../kirby.config').tryInject()\""
  },
  "dependencies": {
    "vue": "3.0.0-rc.5",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "3.0.0-rc.5",
    "cross-env": "^7.0.2",
    "dotenv-flow": "^3.2.0",
    "html-minifier-terser": "^5.1.1",
    "php-server": "^0.2.1",
    "postcss-preset-env": "^6.7.0",
    "vite": "^1.0.0-rc.9"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "postcss": {
    "plugins": {
      "postcss-preset-env": {
        "features": {
          "nesting-rules": true
        }
      }
    }
  }
}
